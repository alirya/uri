{
  "name": "@dikac/t-uri",
  "version": "0.4.3",
  "description": "Typescript value",
  "dependencies": {
    "@dikac/deployer": "^0.1.5",
    "@dikac/t-array": "^0.23.10",
    "@dikac/t-function": "^0.12.4",
    "@dikac/t-message": "^0.10.4",
    "@dikac/t-number": "^0.15.13",
    "@dikac/t-object": "^0.33.4",
    "@dikac/t-string": "^0.14.1",
    "@dikac/t-validatable": "^0.18.9",
    "@dikac/t-validator": "^0.16.6",
    "@dikac/t-value": "^0.12.6",
    "@types/lodash": "^4.14.161",
    "is-domain-name": "^1.0.1",
    "is-ip": "^3.1.0",
    "lodash": "^4.17.20",
    "query-string": "^6.13.1",
    "ts-toolbelt": "^6.15.5",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.14",
    "cpy-cli": "^3.1.1",
    "jasmine": "^3.6.1",
    "jasmine-ts-console-reporter": "^3.1.1",
    "json-properties-remover": "^0.1.2",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-cleaner": "^1.0.5",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "spec-run-js": "jasmine --config=./jasmine.json",
    "spec-run": "jasmine --reporter=jasmine-ts-console-reporter --config=./jasmine.json",
    "spec": "npm-run-all spec-compile spec-run",
    "spec-ensure-directory": "mkdirp dist-spec",
    "spec-compile": "npm-run-all spec-compile-script spec-cleanup",
    "spec-compile-watch": "npm-run-all --sequential spec-ensure-directory --parallel spec-compile-watch-script spec-cleanup-watch",
    "spec-compile-script": "tsc -p tsconfig-spec.json",
    "spec-compile-watch-script": "tsc -w -p tsconfig-spec.json",
    "compile-script": "tsc",
    "compile": "npm-run-all compile-script cleanup",
    "cleanup": "ts-cleaner --verbose --dist=dist --src=src",
    "cleanup-watch": "ts-cleaner --verbose --dist=dist --src=src --watch",
    "spec-cleanup": "ts-cleaner --verbose --dist=dist-spec --src=spec",
    "spec-cleanup-watch": "ts-cleaner --verbose --dist=dist-spec --src=spec --watch",
    "compile-script-watch": "tsc -w",
    "compile-watch": "npm-run-all --sequential compile-ensure-directory --parallel compile-script-watch cleanup-watch",
    "compile-ensure-directory": "mkdirp dist",
    "release": "remove-json-properties dist/package.json private scripts.postinstall scripts.deploy dependencies.@dikac/deployer",
    "postrelease": "npm publish dist",
    "prerelease": "cpy     package.json package-lock.json LICENSE Readme.md .npmignore dist",
    "postpublish": "rimraf package.json package-lock.json LICENSE Readme.md .npmignore",
    "postinstall": "npm run deploy",
    "deploy": "deployer /.git /dist /"
  },
  "author": "",
  "private": true,
  "license": "MIT"
}
